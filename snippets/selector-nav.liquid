
<ul class="selector-nav">
  {% for block in section.blocks %}
    {% if block.type == 'featured_collection' %}
    <li class="selector-nav-item" data-collectionurl="{{block.settings.collection.url}}" {{ block.shopify_attributes }}>
        {{block.settings.collection.title}}
    </li>
    {% endif %}
  {% endfor %}
</ul>
